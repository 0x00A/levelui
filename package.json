{
  "name": "levelweb",
  "version": "1.0.0",
  "description": "A LevelDB GUI. Includes simple data visualization tools. See also the [`commandline version`][99].",
  "main": "index.js",
  "scripts": {
    "exec": "npm run styles && npm run layouts && node_modules/.bin/atom-shell .",
    "styles": "node_modules/.bin/stylus ./styles/index.styl -o ./assets/css",
    "layouts": "node_modules/.bin/jade ./layouts/index.jade -o ./assets/html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hij1nx/levelweb.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hij1nx/levelweb/issues"
  },
  "homepage": "https://github.com/hij1nx/levelweb",
  "dependencies": {
    "atom-shell": "^1.1.7",
    "codemirror": "^4.8.0",
    "dom-events": "^0.1.1",
    "domready": "^1.0.7",
    "jade": "^1.7.0",
    "leveldown": "^1.0.0",
    "stylus": "^0.49.3"
  }
}
